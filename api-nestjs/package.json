{
  "name": "marketplace-api",
  "version": "1.0.0",
  "description": "Enterprise marketplace API with advanced search",
  "author": "Marketplace Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "search:setup": "ts-node scripts/search-setup.ts",
    "search:reindex": "ts-node scripts/search-reindex.ts",
    "search:reindex:force": "ts-node scripts/search-reindex.ts -- --force",
    "search:reindex:dry-run": "ts-node scripts/search-reindex.ts -- --dry-run",
    "search:deploy": "bash scripts/deploy-search.sh",
    "search:deploy:prod": "bash scripts/deploy-search.sh production",
    "search:deploy:dry-run": "bash scripts/deploy-search.sh development false true",
    "search:health": "curl -s http://localhost:3000/api/search/health | jq",
    "search:test": "npm run test -- --testPathPattern=search",
    "notification:seed": "ts-node scripts/seed-notification-templates.ts",
    "notification:test": "npm run test -- --testPathPattern=notification",
    "notification:health": "curl -s http://localhost:3001/api/notifications/health | jq",
    "notification:check": "ts-node scripts/check-notification-system.ts",
    "infra:up": "docker-compose -f infrastructure/docker-compose.search.yml up -d",
    "infra:down": "docker-compose -f infrastructure/docker-compose.search.yml down",
    "infra:logs": "docker-compose -f infrastructure/docker-compose.search.yml logs -f",
    "infra:restart": "npm run infra:down && npm run infra:up"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.958.0",
    "@aws-sdk/client-sns": "^3.958.0",
    "@elastic/elasticsearch": "^8.11.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bull": "^10.2.3",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/terminus": "^10.3.0",
    "@nestjs/throttler": "^5.0.1",
    "@prisma/client": "^7.2.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "bullmq": "^5.66.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "decimal.js": "^10.6.0",
    "dotenv": "^16.6.1",
    "firebase-admin": "^13.6.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.2",
    "nodemailer": "^7.0.12",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^7.2.0",
    "razorpay": "^2.9.6",
    "redis": "^4.7.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "stripe": "^17.7.0",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^5.11.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.20",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^4.10.4",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/decimal.js": "^0.0.32",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^7.0.4",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
